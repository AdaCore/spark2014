project IPStack_Config is
   Gen_Prefix := "../build/gen/";
   Obj_Prefix := "../build/obj/";
   Lib_Prefix := "../build/lib/";
   ALI_Prefix := "../build/ali/";

   package Compiler is
      Common_Switches := ("-g");
      for Default_Switches ("Ada")
        use Common_Switches & ("-gnat05", "-gnatwa", "-gnatyg", "-gnata");
