 function index_Index_one
   :int =
  1

  function bool_eq (a:map) (af:int) (al:int) (b:map) (bf:int) (bl:int) : bool =
   (if af <= al
        then al - af = bl - bf
        else bf > bl) /\
         (forall idx : int. af <= idx <= al ->
             (get a idx) = (get b (bf - af + idx)))

