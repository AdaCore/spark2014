with "sparklib";

project Test is

   for Main use ("test.adb");

   package Builder is
      for Switches ("ada") use ("-gnata", "-gnat2022");
      for Global_Configuration_Pragmas use "pragmas.adc";
   end Builder;

   package Compiler is
      for Switches ("ada") use ("-g");
   end Compiler;

   package Linker is
      for Switches ("ada") use ("-g");
   end Linker;
   
end Test;

