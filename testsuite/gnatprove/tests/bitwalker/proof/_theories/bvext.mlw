module Bvext__eq
 use        "_gnatprove_standard".Main
 use        "_gnatprove_standard".BV64

 predicate eq__logic (a : BV64.t) (b : BV64.t)
          = (a = b)

 val eq (a : BV64.t) (b : BV64.t) : bool
  requires { true }
  ensures { (result = (eq__logic a b)) }
end
