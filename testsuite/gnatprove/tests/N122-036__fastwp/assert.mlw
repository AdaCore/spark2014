module Assert
  use import int.Int
  use import ref.Ref

(* Valid *)
  let test1 (x : ref int)
    requires { true }
    ensures { true }
    =
    if !x > 1 then
      assert { !x > 0 }
    else
      assert { !x <= 1 }

(* Unknown *)
  let test2 (x : ref int)
    requires { true }
    ensures { true }
    =
    if !x > 1 then
      assert { !x <= 1 }
    else
      assert { !x > 0 }
end
