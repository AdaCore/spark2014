module An
   use import int.Int
   use import ref.Ref

(* Valid *)
   let f (x : int)
      requires { true }
      ensures { x = result }
      =
         any int
         requires { true }
         ensures { result = x }

(* Unknown *)
   let g (x : int)
      requires { true }
      ensures { result = 0 }
      =
      any int requires { true } ensures {result > 0 }

end
