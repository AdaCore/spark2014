(* function "=" on vectors *)
module Ada__containers__formal_indefinite_vectors__Oeq__2
  use import int.Int
  use import "_gnatprove_standard".Main
  use        "ada__containers__formal_indefinite_vectors__args".Ada__containers__formal_indefinite_vectors__element_type
  use        "ada__containers__formal_indefinite_vectors__args".Ada__containers__formal_indefinite_vectors__oeq
