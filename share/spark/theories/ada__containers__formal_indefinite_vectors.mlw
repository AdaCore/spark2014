(* procedure Append on vectors *)
module Ada__containers__formal_indefinite_vectors__append
  use import int.Int
  use import "_gnatprove_standard".Main
  use import Ada__containers__formal_indefinite_vectors__vector
  use import Ada__containers__formal_indefinite_vectors__capacity
  use import Ada__containers__formal_indefinite_vectors__length
  use        "ada__containers__formal_indefinite_vectors__args".Ada__containers__formal_indefinite_vectors__bounded
  use import Ada__containers__formal_indefinite_vectors__element

  predicate append vector vector vector

  axiom append_length_:
    forall v1 v2 nv : vector [append v1 nv v2].
	append v1 nv v2 -> length_ v2 = length_ v1 + length_ nv

  axiom append_capacity:
    forall v1 v2 nv : vector [append v1 nv v2].
      Ada__containers__formal_indefinite_vectors__bounded.bounded -> append v1 nv v2 ->
        capacity v1 = capacity v2

  axiom append_element1:
    forall v1 v2 nv : vector,
           j        : int
           [append v1 nv v2, element_ v1 j | append v1 nv v2, element_ v2 j].
      append v1 nv v2 -> 0 < j <= length_ v1 -> element_ v1 j = element_ v2 j

  axiom append_element2:
    forall v1 v2 nv : vector, j : int [append v1 nv v2, element_ nv j].
      append v1 nv v2 -> 0 < j <= length_ nv -> element_ nv j = element_ v2 (length_ v1 + j)

  axiom append_element2bis:
    forall v1 v2 nv : vector, j : int [append v1 nv v2, element_ v2 j].
      append v1 nv v2 ->
        length_ v1 < j <= length_ v1 + length_ nv ->
          element_ nv (j - length_ v1) = element_ v2 j

  val append (v:Ada__containers__formal_indefinite_vectors__vector.__split_fields__ref) (v_cap : Ada__containers__formal_indefinite_vectors__vector.__split_discrs) (nv : vector) : unit
     requires { let rec_v = {Ada__containers__formal_indefinite_vectors__vector.__split_fields = v.Ada__containers__formal_indefinite_vectors__vector.__split_fields__content;
                              Ada__containers__formal_indefinite_vectors__vector.__split_discrs = v_cap}  in
                   Ada__containers__formal_indefinite_vectors__bounded.bounded ->
                  length_ rec_v + length_ nv <= capacity rec_v }
     writes   { v }
     reads    { v }
     ensures  { let rec_v = {Ada__containers__formal_indefinite_vectors__vector.__split_fields = v.Ada__containers__formal_indefinite_vectors__vector.__split_fields__content;
                              Ada__containers__formal_indefinite_vectors__vector.__split_discrs = v_cap}  in
               let old_v = {Ada__containers__formal_indefinite_vectors__vector.__split_fields = old v.Ada__containers__formal_indefinite_vectors__vector.__split_fields__content;
                              Ada__containers__formal_indefinite_vectors__vector.__split_discrs = v_cap}  in
                   append old_v nv rec_v }
end

