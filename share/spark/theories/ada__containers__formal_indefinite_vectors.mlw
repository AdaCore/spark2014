(* procedure Swap *)
module Ada__containers__formal_indefinite_vectors__swap
  use import int.Int
  use import "_gnatprove_standard".Main
  use import Ada__containers__formal_indefinite_vectors__index_type_helper
  use import Ada__containers__formal_indefinite_vectors__vector
  use import Ada__containers__formal_indefinite_vectors__capacity
  use import Ada__containers__formal_indefinite_vectors__length
  use import Ada__containers__formal_indefinite_vectors__element

  predicate swap vector int int vector

  axiom swap_len_cap :
    forall v1 v2 : vector, i1 i2 : int [swap v1 i1 i2 v2 ].
      swap v1 i1 i2 v2 ->
        length_ v1 = length_ v2 /\
        capacity v1 = capacity v2

  axiom swap_elt:
    forall v1 v2    : vector,
           i1 i2, i : int
           [swap v1 i1 i2 v2, element_ v1 i|swap v1 i1 i2 v2, element_ v2 i ].
      swap v1 i1 i2 v2 -> 0 < i <= length_ v1 -> i <> i1 /\ i <> i2 ->
        element_ v1 i = element_ v2 i

  axiom swap_range:
    forall v1 v2 : vector, i1 i2 : int [swap v1 i1 i2 v2 ].
      swap v1 i1 i2 v2 ->
        0 < i1 <= length_ v1 /\
        element_ v1 i1 = element_ v2 i2 /\
        0 < i2 <= length_ v1 /\
        element_ v1 i2 = element_ v2 i1

  val swap (v:Ada__containers__formal_indefinite_vectors__vector.__split_fields__ref) (v_cap : Ada__containers__formal_indefinite_vectors__vector.__split_discrs) (i1 i2 : int) : unit
    requires { let rec_v = {Ada__containers__formal_indefinite_vectors__vector.__split_fields = v.Ada__containers__formal_indefinite_vectors__vector.__split_fields__content;
                              Ada__containers__formal_indefinite_vectors__vector.__split_discrs = v_cap}  in
                   0 < index_type__to_index i1 <= length_ rec_v /\
                   0 < index_type__to_index i2 <= length_ rec_v }
     writes  { v }
     reads   { v }
     ensures { let rec_v = {Ada__containers__formal_indefinite_vectors__vector.__split_fields = v.Ada__containers__formal_indefinite_vectors__vector.__split_fields__content;
                              Ada__containers__formal_indefinite_vectors__vector.__split_discrs = v_cap}  in
               let old_v = {Ada__containers__formal_indefinite_vectors__vector.__split_fields = old v.Ada__containers__formal_indefinite_vectors__vector.__split_fields__content;
                              Ada__containers__formal_indefinite_vectors__vector.__split_discrs = v_cap}  in
                   (i1 = i2 /\ v.Ada__containers__formal_indefinite_vectors__vector.__split_fields__content = (old v.Ada__containers__formal_indefinite_vectors__vector.__split_fields__content)) \/
                   (i1 <> i2 /\ swap old_v (index_type__to_index i1) (index_type__to_index i2) rec_v) }
end

