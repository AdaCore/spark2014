  use import "_gnatprove_standard".Main
  use        Ada__containers__count_type
  use        "ada__containers__formal_indefinite_vectors__args".Ada__containers__formal_indefinite_vectors__index_type

  function index_type_length "inline" : int =
    Ada__containers__formal_indefinite_vectors__index_type.last -
    Ada__containers__formal_indefinite_vectors__index_type.first + 1

  axiom index_type_length :
    Ada__containers__count_type.in_range (index_type_length)

  function index_type__to_index "inline" (i : int) : int =
    i - Ada__containers__formal_indefinite_vectors__index_type.first + 1

  function index_type__of_index "inline" (i : int) : int =
    i + Ada__containers__formal_indefinite_vectors__index_type.first - 1
end

